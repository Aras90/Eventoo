      <id column="Id_User" name="Id_User" type = "long"></id>
      <property column="Description" name ="Description" type = "string" not-null="true"></property>
      <property column="Password" name ="Password" type = "string" not-null="true"></property>
      <property column="CreatedAt" name ="CreatedAt" type = "string"></property>
      <property column="EditetAt" name ="EditetAt" type = "string"></property>
      <property column="Email" name ="Email" type = "string"></property>
  </class>
</hibernate-mapping>